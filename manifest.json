{
  "manifest_version": 2,

  "name": "DAASH",
  "description": "This extension shows you your company's SaaS products",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["jquery.js", "livesearch.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "newtab.html"
  },
  "chrome_url_overrides":{
  "newtab" : "home.html"
  },
  "icons" : {"128" : "icon.png"}
}